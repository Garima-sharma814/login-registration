 <div>
    <form action= "<%= formaction %>" method="post">
        <div style="margin-left: 32px;"><label for="title">Name:</label>
        <input type="text" name="username" placeholder="Enter your name" /></div>
        <div style="margin-left: 32px;"><label for="email">Email:</label>
        <input  type="email" name="email" placeholder="<%= email %>" id="title"/></div>
        <div><label for="password">Password:</label>
        <input type="password" placeholder="Enter password" name="password"></input></div>
        <div><label for="userrole">User Role:</label>
        <input type="text" placeholder="<%= role %>" name="role"></input></div>
        <br>
        <button class="btn" type="submit"> <%= btn %></button>
    </form>
</div>